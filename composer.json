{
    "name": "scriptotek/bibrex",
    "description": "Bibrex lending system",
    "keywords": ["lending system"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.2.0",
        "ext-imagick": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "beyondcode/laravel-self-diagnosis": "^1.0",
        "danielstjules/stringy": "^3.1",
        "doctrine/dbal": "^2.7",
        "fideloper/proxy": "^4.0",
        "fruitcake/laravel-cors": "^1.0",
        "guzzlehttp/guzzle": "~6.0",
        "http-interop/http-factory-guzzle": "^1.0",
        "imagine/imagine": "^1.2",
        "jenssegers/agent": "^2.6",
        "laravel/framework": "^6.0",
        "laravel/helpers": "^1.1",
        "laravelcollective/html": "^6.0",
        "php-http/guzzle6-adapter": "^2.0",
        "pusher/pusher-php-server": "~4.0",
        "scriptotek/alma-client": "dev-master",
        "sentry/sentry-laravel": "^1.1",
        "zircote/swagger-php": "^3.0"
    },
    "require-dev": {
        "balping/artisan-bash-completion": "^1.0",
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.6",
        "deployer/deployer": "^6.2",
        "deployer/recipes": "^6.1",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "laravel/dusk": "^5.0",
        "laravel/tinker": "^2.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0",
        "squizlabs/php_codesniffer": "^3.3"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
